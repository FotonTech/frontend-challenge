        "eslint-plugin-react-hooks": "4.2.0",
        "html-webpack-plugin": "5.3.1",
        "husky": "4.3.6",
        "lint-staged": "11.0.0",
        "prettier": "2.3.0",
        "terser-webpack-plugin": "5.1.3",
        "ts-loader": "9.2.2",
        "ts-node": "10.0.0",
        "typescript": "4.3.2",
        "unused-webpack-plugin": "2.4.0",
        "webpack": "5.38.1",
        "webpack-cli": "4.7.0",
        "webpack-dev-server": "3.11.2",
        "webpack-manifest-plugin": "3.1.1"
    },
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
